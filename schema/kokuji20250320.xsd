        <xs:element ref="Sub"/>
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <!--  Remarks =============================================================== -->
  <xs:element name="Remarks">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="RemarksLabel"/>
        <xs:choice>
          <xs:element maxOccurs="unbounded" ref="Item"/>
          <xs:element maxOccurs="unbounded" ref="Sentence"/>
        </xs:choice>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="RemarksLabel">
    <xs:complexType mixed="true">
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="Line"/>
        <xs:element ref="Ruby"/>
        <xs:element ref="Sup"/>
        <xs:element ref="Sub"/>
      </xs:choice>
      <xs:attribute name="LineBreak" type="xs:boolean" default="false"/>
    </xs:complexType>
  </xs:element>
  <!--  List ================================================================== -->
  <xs:element name="List">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="ListSentence"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="Sublist1"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="ListSentence">
    <xs:complexType>
      <xs:choice>
        <xs:element maxOccurs="unbounded" ref="Sentence"/>
        <xs:element maxOccurs="unbounded" ref="Column"/>
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <!--  Sublist level1========================================================= -->
  <xs:element name="Sublist1">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="Sublist1Sentence"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="Sublist2"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="Sublist1Sentence" type="ListSentenceType"/>
  <!--  Sublist level2========================================================= -->
  <xs:element name="Sublist2">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="Sublist2Sentence"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="Sublist3"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="Sublist2Sentence" type="ListSentenceType"/>
  <!--  Sublist level3========================================================= -->
  <xs:element name="Sublist3">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="Sublist3Sentence"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element> 
  <xs:element name="Sublist3Sentence" type="ListSentenceType"/>
  <!--  QuoteStruct =========================================================== -->
  <xs:element name="QuoteStruct" type="any"/>
  <!--  Ruby ================================================================== -->
  <xs:element name="Ruby">
    <xs:complexType mixed="true">
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="Rt"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="Rt" type="xs:string"/>
  <!--  Line ================================================================== -->
  <xs:element name="Line">
    <xs:complexType mixed="true">
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="QuoteStruct"/>
        <xs:element ref="ArithFormula"/>
        <xs:element ref="Ruby"/>
        <xs:element ref="Sup"/>
        <xs:element ref="Sub"/>
        <xs:element ref="ExternalLink" />
      </xs:choice>
      <xs:attribute name="Style" default="solid">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="dotted"/>
            <xs:enumeration value="double"/>
            <xs:enumeration value="none"/>
            <xs:enumeration value="solid"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <!--  Sup =================================================================== -->
  <xs:element name="Sup" type="xs:string"/>
  <!--  Sub =================================================================== -->
  <xs:element name="Sub" type="xs:string"/>
  <!--  ExternalLink =================================================================== -->
  <xs:element name="ExternalLink">
    <xs:complexType mixed="true">
      <xs:attribute name="Type">
        <xs:simpleType>
            <xs:restriction base="xs:token">
                <xs:enumeration value="LawID"/>
                <xs:enumeration value="URI"/>
                <xs:enumeration value="Other"/>
            </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="Link" type="xs:string"></xs:attribute>
    </xs:complexType>
  </xs:element>

  <!-- 
  ************************************************************************
  **
  ** Type Definitions
  ** Define commonly used types
  **
  **
  ************************************************************************
  -->

  <!-- List Sentence Type -->
  <xs:complexType name="ListSentenceType">
    <xs:choice>
      <xs:element maxOccurs="unbounded" ref="Sentence"/>
      <xs:element maxOccurs="unbounded" ref="Column"/>
    </xs:choice>
  </xs:complexType>

  <!-- Item Sentence Type -->
  <xs:complexType name="ItemSentenceType">
    <xs:choice>
      <xs:element maxOccurs="unbounded" ref="Sentence"/>
      <xs:element maxOccurs="unbounded" ref="Column"/>
      <xs:element ref="Table"/>
    </xs:choice>
  </xs:complexType>

  <!-- Simple Title Type -->
  <xs:complexType name="SimpleTitleType" mixed="true">
    <xs:choice minOccurs="0" maxOccurs="unbounded">
      <xs:element ref="Line"/>
      <xs:element ref="Ruby"/>
      <xs:element ref="Sup"/>
      <xs:element ref="Sub"/>
    </xs:choice>
  </xs:complexType>

  <!-- Type with Writing Mode -->
  <xs:complexType name="TitleWithWriingModeType" mixed="true">
    <xs:choice minOccurs="0" maxOccurs="unbounded">
      <xs:element ref="Line"/>
      <xs:element ref="Ruby"/>
      <xs:element ref="Sup"/>
      <xs:element ref="Sub"/>
    </xs:choice>
    <xs:attribute ref="WritingMode" default="vertical"/>
  </xs:complexType>

  <!-- Border Style Type -->
  <xs:simpleType name="BorderStyleType">
    <xs:restriction base="xs:token">
      <xs:enumeration value="solid"/>
      <xs:enumeration value="none"/>
      <xs:enumeration value="dotted"/>
      <xs:enumeration value="double"/>
    </xs:restriction>
  </xs:simpleType>

  <!-- Align Type -->
  <xs:simpleType name="AlignType">
    <xs:restriction base="xs:token">
      <xs:enumeration value="left"/>
      <xs:enumeration value="center"/>
      <xs:enumeration value="right"/>
      <xs:enumeration value="justify"/>
    </xs:restriction>
  </xs:simpleType>

  <!-- Vertical Align Type -->
  <xs:simpleType name="VAlignType">
    <xs:restriction base="xs:token">
      <xs:enumeration value="top"/>
      <xs:enumeration value="middle"/>
      <xs:enumeration value="bottom"/>
    </xs:restriction>
  </xs:simpleType>

  <!-- any -->
  <xs:complexType name="any" mixed="true">
    <xs:sequence>
      <xs:any minOccurs="0" maxOccurs="unbounded" processContents="strict"/>
    </xs:sequence>
  </xs:complexType>

  <!-- 
  ************************************************************************
  **
  ** Common Attribute Definitions
  ** 
  **
  **
  ************************************************************************
  -->

  <!-- WritingMode Attribute -->
  <xs:attribute name="WritingMode">
    <xs:simpleType>
      <xs:restriction base="xs:token">
        <xs:enumeration value="vertical"/>
        <xs:enumeration value="horizontal"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:attribute>

  <!-- 
  ************************************************************************
  **
  ** Extension for Kokuji
  ** 
  **
  **
  ************************************************************************
  -->


  <!--  Num =============================================================== -->
  <xs:element name="Num" type="xs:string"/>
  <!--  Promulgation =============================================================== -->
  <xs:element name="Promulgation">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="PromulgateStatement" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="PromulgateBody" minOccurs="0" maxOccurs="1" />
        <xs:element ref="PromulgateDate" minOccurs="0" maxOccurs="1" />
        <xs:element ref="Signature" minOccurs="0" maxOccurs="1"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!--  PromulgateStatement =============================================================== -->
  <xs:element name="PromulgateStatement" >
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="GeneralParagraph"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element> 
  <!--  PromulgateBody =============================================================== -->
  <xs:element name="PromulgateBody" >
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="GeneralParagraph"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!--  PromulgateDate =============================================================== -->
  <xs:element name="PromulgateDate">
    <xs:complexType mixed="true">
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="Ruby"/>
      </xs:choice>
      <xs:attribute name="Appearance" type="xs:string" />
    </xs:complexType>
  </xs:element>
  <!--  SignatureStruct =============================================================== -->
  <xs:element name="Signature">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="PublicPosition" />
        <xs:element ref="Name"/>
      </xs:sequence>
      <xs:attribute name="Appearance" type="xs:string" />
    </xs:complexType>
  </xs:element>
  <!--  MinisterialTitle =============================================================== -->
  <xs:element name="PublicPosition">
    <xs:complexType mixed="true">
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="Ruby"/>
      </xs:choice>
      <xs:attribute name="Appearance" type="xs:string" />
    </xs:complexType>
  </xs:element>
  <!--  Signature =============================================================== -->
  <xs:element name="Name">
    <xs:complexType mixed="true">
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="Ruby"/>
      </xs:choice>
      <xs:attribute name="Appearance" type="xs:string" />
    </xs:complexType>
  </xs:element>
  <!-- Title -->
  <xs:element name="Title">
    <xs:complexType mixed="true">
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="Line"/>
        <xs:element ref="Ruby"/>
        <xs:element ref="Sup"/>
        <xs:element ref="Sub"/>
      </xs:choice>
      <xs:attribute name="Appearance" type="xs:string" />
    </xs:complexType>
  </xs:element>
  <!--  Block ================================================================= -->
  <xs:element name="Block">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="BlockProperties" minOccurs="0"/>
        <xs:element ref="Num" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="Title" minOccurs="0"/>
        <xs:choice maxOccurs="unbounded">
          <xs:element ref="Block"/>
          <xs:element ref="Section"/>
          <xs:element ref="Paragraph"/>
          <xs:element ref="GeneralParagraph"/>
        </xs:choice>
        <xs:element minOccurs="0" ref="Note"/>
      </xs:sequence>
      <xs:attribute name="Num" use="required" type="xs:positiveInteger"/>
      <xs:attribute name="Delete" type="xs:boolean" default="false"/>
      <xs:attribute name="Hide" type="xs:boolean" default="false"/>
      <xs:attribute name="Appearance" type="xs:string" />
    </xs:complexType>
  </xs:element>
  <!--  BlockProperties ================================================================= -->
  <xs:element name="BlockProperties">
    <xs:complexType>
      <xs:attribute name="BlockType" use="required" type="xs:token"></xs:attribute>
      <xs:attribute name="Appearance" type="xs:string" />
    </xs:complexType>
  </xs:element>
  <!--  NumberedList ================================================================= -->
  <xs:element name="NumberedList">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="ListItem" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute name="Appearance" type="xs:string" />
    </xs:complexType>
  </xs:element>

  <!--  UnnumberedList ================================================================= -->
  <xs:element name="UnnumberedList">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="ListItem" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute name="Appearance" type="xs:string" />
    </xs:complexType>
  </xs:element>

  <!--  ListItem ================================================================= -->
  <xs:element name="ListItem">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="Num" minOccurs="0"/>
        <xs:choice minOccurs="0" maxOccurs="unbounded">
          <xs:element ref="GeneralParagraph"/>
          <xs:element ref="Sentence"/>
          <xs:element minOccurs="0" maxOccurs="unbounded" ref="NumberedList"/>
          <xs:element minOccurs="0" maxOccurs="unbounded" ref="UnnumberedList"/>
        </xs:choice>
      </xs:sequence>
      <xs:attribute name="Num" />
      <xs:attribute name="Appearance" type="xs:string" />
    </xs:complexType>
  </xs:element>

  <!--  GeneralParagraph ========================================================= -->
  <xs:element name="GeneralParagraph">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element ref="Sentence"/>
        <xs:element ref="NumberedList" />
        <xs:element ref="UnnumberedList" />
        <xs:element ref="TableStruct" />
        <xs:element ref="FigStruct" />
        <xs:element ref="StyleStruct" />
        <xs:element ref="FormatStruct" />
      </xs:choice>
      <xs:attribute name="Num" />
      <xs:attribute name="Appearance" type="xs:string" />
    </xs:complexType>
  </xs:element>

  <!--  Chars ================================================================= -->
  <xs:element name="Chars">
    <xs:complexType mixed="true">
      <xs:attribute name="Appearance" type="xs:string" />
    </xs:complexType>
  </xs:element>

  <!--  Appendix ================================================================= -->
  <xs:element name="Appendix">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="Block" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>
